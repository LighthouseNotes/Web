@inherits DeleteUserConfirmationBase
<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.body1"> Are you sure you want to delete the user @DisplayName? </MudText>
        <MudText Typo="Typo.caption"> This action is not recoverable! </MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="@Cancel">No</MudButton>
        <MudButton Color="Color.Error" OnClick="@Submit">Yes</MudButton>
    </DialogActions>
</MudDialog>